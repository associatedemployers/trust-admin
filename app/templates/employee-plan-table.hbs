<thead>
<tr>
  <th></th>
  {{#each levels in coverage}}
    <th>{{title-case levels.type}}</th>
  {{/each}}
</tr>
</thead>
<tbody>
{{! Employee}}
<tr>
  <th>Employee</th>
  {{#each levels in coverage}}
    <td>
      {{#if levels.tableLevel.employee}}
        <i class="fa fa-check text-success"></i>
      {{else}}
        <i class="fa fa-times text-danger"></i>
      {{/if}}
    </td>
  {{/each}}
</tr>
{{! Spouse}}
<tr {{bind-attr class="content.isMarried::disabled-strike"}}>
  <th>Spouse</th>
  {{#each levels in coverage}}
    <td>
      {{#if levels.tableLevel.spouse}}
        <i class="fa fa-check text-success"></i>
      {{else}}
        <i class="fa fa-times text-danger"></i>
      {{/if}}
    </td>
  {{/each}}
</tr>
{{! Children}}
<tr>
  <th>Children</th>
  {{#each levels in coverage}}
    <td>
      {{#if levels.tableLevel.children}}
        <i class="fa fa-check text-success"></i>
      {{else}}
        <i class="fa fa-times text-danger"></i>
      {{/if}}
    </td>
  {{/each}}
</tr>
</tbody>
