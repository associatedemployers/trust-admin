<div class="row">
{{log content}}
  <div class="col-md-6">
    <h3>About</h3>
    <ul class="no-list-style">
      <li><i {{bind-attr class=":fa :fa-fw content.super:fa-check:fa-times"}}></i> Super user</li>
      <li><i {{bind-attr class=":fa :fa-fw content.receiveEmails:fa-check:fa-times"}}></i> Receive Emails</li>
      <li><i {{bind-attr class=":fa :fa-fw content.apiAccess:fa-check:fa-times"}}></i> Direct API Access</li>
    </ul>
  </div>
  <div class="col-md-6">
    <h3>Permissions</h3>
    <ul class="no-list-style">
      {{#each permission in permissionsDisplay}}
        <li>
          Can
          {{#each type in permission.types}}
            <span class="label label-default">{{type}}</span>
          {{/each}}
          {{permission.name}}
        </li>
      {{else}}
        <i class="fa fa-fw fa-exclamation-triangle"></i> This user does not have permissions.
      {{/each}}
    </ul>
  </div>
</div>
<hr />
<div class="row">
  <div class="col-md-12">
    <h3 class="text-center">Activity</h3>
    <p class="text-center text-muted">
      Some sort of activity feed
    </p>
  </div>
</div>
